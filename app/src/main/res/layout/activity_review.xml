        android:layout_marginHorizontal="5dp"
        android:layout_marginVertical="5dp"
        android:layout_marginTop="50dp"
        app:cardBackgroundColor="#FFFF"
        app:cardCornerRadius="20dp"
        app:layout_constraintTop_toBottomOf="@+id/JenisWisata"
        tools:layout_editor_absoluteX="5dp"><![CDATA[>

        ]]>

        <ImageView
            android:id="@+id/gambar1"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:layout_marginStart="20dp"
            android:layout_marginTop="15dp"
            app:srcCompat="@drawable/gambar1" />

        <TextView
            android:id="@+id/namaMenu"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="10dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/quicksand"
            android:text="Sate Kelinci"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/alamat"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginLeft="150dp"
            android:layout_marginTop="35dp"
            android:layout_marginEnd="10dp"
            android:layout_marginRight="10dp"
            android:fontFamily="@font/quicksand"
            android:text="Batu, Malang"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/rating"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginTop="75dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/quicksand"
            android:text="Rating : 5"
            android:textColor="#000000"
            android:textSize="15sp" />

        <TextView
            android:id="@+id/review"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="150dp"
            android:layout_marginTop="95dp"
            android:layout_marginEnd="10dp"
            android:fontFamily="@font/quicksand"
            android:text="Review : Enak Bangett"
            android:textColor="#000000"
            android:textSize="15sp" />

        <ImageButton
